<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
   android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".FinishedBookingDetail">
    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:id="@+id/rellayout"
        android:background="#fff">


        <TextView
            android:layout_width="wrap_content"
            android:textColor="@color/Black"
            android:textSize="20sp"
            android:layout_height="wrap_content"
            android:text="New Text"
            android:id="@+id/Datetime"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/Black"
            android:textSize="20sp"

            android:text="New Text"
            android:id="@+id/crn"
            android:layout_below="@+id/Datetime"
            android:layout_centerHorizontal="true" />
        <View
            android:layout_width="fill_parent"
            android:layout_height="2sp"
            android:background="@color/LightGrey"
            android:layout_below="@+id/crn"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="30dp"
            android:id="@+id/view" />
        <View
            android:layout_width="fill_parent"
            android:layout_height="2sp"
            android:background="@color/LightGrey"

            android:id="@+id/view1"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Driver Name:"
            android:id="@+id/textView2"
            android:layout_marginTop="44dp"
            android:layout_below="@+id/view"
            android:layout_alignLeft="@+id/textView3"
            android:layout_alignStart="@+id/textView3"
            android:layout_marginBottom="20dp"
            android:textColor="@color/Black"
            android:textSize="12sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Mobile No.:"
            android:id="@+id/textView3"
            android:layout_marginLeft="36dp"
            android:layout_marginStart="36dp"
            android:layout_below="@+id/textView2"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="20dp"
            android:textColor="@color/Black"
            android:textSize="12sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Truck:"
            android:id="@+id/textView4"
            android:layout_below="@+id/textView3"
            android:layout_alignLeft="@+id/textView3"
            android:layout_alignStart="@+id/textView3"
            android:textColor="@color/Black"
            android:textSize="12sp" />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/truck"
            android:text="New Text"
            android:textColor="@color/Black"
            android:layout_alignTop="@+id/textView4"
            android:layout_alignLeft="@+id/mobno"
            android:layout_alignStart="@+id/mobno"
            android:textSize="12sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New Text"
            android:id="@+id/mobno"
            android:textColor="@color/Black"
            android:layout_below="@+id/textView2"
            android:layout_alignLeft="@+id/drivername"
            android:layout_alignStart="@+id/drivername"
            android:textSize="12sp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New Text"
            android:id="@+id/drivername"
            android:textColor="@color/Black"
            android:layout_alignTop="@+id/textView2"
            android:layout_toRightOf="@+id/fare"
            android:layout_toEndOf="@+id/fare"
            android:layout_marginLeft="20sp"
            android:textSize="12sp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView"5
            android:src="@drawable/addcontact"
            android:layout_toRightOf="@+id/drivername"
            android:layout_above="@+id/mobno"
            android:layout_alignTop="@+id/drivername"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView2"
            android:src="@drawable/vehicle_1"
            android:textColor="@color/Black"
            android:layout_below="@+id/mobno"
            android:layout_alignRight="@+id/imageView"
            android:layout_alignEnd="@+id/imageView"
            android:layout_alignBottom="@+id/truck"
            android:layout_alignLeft="@+id/imageView"
            android:layout_alignStart="@+id/imageView" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Booking Details"
            android:id="@+id/textView5"
            android:layout_alignTop="@+id/view1"
            android:layout_alignLeft="@+id/textView4"
            android:layout_alignStart="@+id/textView4"
            android:textSize="15sp"
            android:textColor="@color/Black"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New Text"
            android:id="@+id/pickup"
            android:layout_below="@+id/textView5"
            android:layout_alignLeft="@+id/textView5"
            android:layout_alignStart="@+id/textView5"
            android:layout_marginTop="50dp"
            android:textSize="13sp"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:textColor="@color/Black"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New Text"
            android:textSize="13sp"
            android:id="@+id/drop"
            android:layout_below="@+id/pickup"
            android:layout_alignLeft="@+id/pickup"
            android:layout_alignStart="@+id/pickup"
            android:layout_marginTop="32dp"
            android:layout_alignRight="@+id/imageView2"
            android:layout_alignEnd="@+id/imageView2"
            android:textColor="@color/Black"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="FARE"
            android:id="@+id/fare"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="32dp"
            android:textColor="@color/Black"
            android:textStyle="bold"
            />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView3"
            android:layout_above="@+id/drop"
            android:src="@drawable/waypointgreen"
            android:layout_alignTop="@+id/pickup"
            android:layout_toLeftOf="@+id/pickup"
            android:layout_toStartOf="@+id/pickup" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView4"
            android:src="@drawable/dots"
            android:layout_below="@+id/pickup"
            android:layout_above="@+id/imageView5"
            android:layout_alignRight="@+id/imageView3"
            android:layout_alignEnd="@+id/imageView3" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView5"
            android:src="@drawable/waypointred"
            android:layout_alignBottom="@+id/drop"
            android:layout_toLeftOf="@+id/drop"
            android:layout_toStartOf="@+id/drop"
            android:layout_alignTop="@+id/drop" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Driver Details"
            android:id="@+id/textView6"
            android:layout_below="@+id/view"
            android:layout_alignLeft="@+id/textView2"
            android:layout_alignStart="@+id/textView2"
            android:textSize="15sp"
            android:textColor="@color/Black"
            android:textStyle="bold"/>
    </RelativeLayout>
</FrameLayout>
